{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": [
      "@cloudflare/workers-types"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "src/types.d.ts",
    "content"
  ]
}